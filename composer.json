{
    "name": "qubus/injector",
    "homepage": "https://github.com/QubusPHP/injector",
    "description": "A dependency injector for bootstrapping object-oriented PHP applications.",
    "keywords": [
        "dependency injection",
        "dic",
        "ioc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Parker",
            "email": "joshua@joshuaparker.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "friendsofphp/proxy-manager-lts": "^1",
        "psr/container": "^2",
        "qubus/config": "^3",
        "qubus/exception": "^3"
    },
    "require-dev": {
        "mockery/mockery": "1.3.1",
        "phpunit/phpunit": "^9.0",
        "qubus/qubus-coding-standard": "^1.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Qubus\\Injector\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qubus\\Tests\\Injector\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
