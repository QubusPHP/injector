{
    "name": "qubus/injector",
    "homepage": "https://github.com/QubusPHP/injector",
    "description": "A dependency injector for bootstrapping object-oriented PHP applications.",
    "keywords": [
        "dependency injection",
        "dic",
        "ioc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Parker",
            "email": "josh@joshuaparker.blog",
            "role": "Developer",
            "homepage": "https://joshuaparker.dev/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ocramius/proxy-manager": "^2.10",
        "psr/container": "^1.0",
        "qubus/exception": "^1.1"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.0",
        "mockery/mockery": "1.3.1",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.3.1"
    },
    "autoload": {
        "psr-4": {
            "Qubus\\Injector\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qubus\\Tests\\Injector\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
